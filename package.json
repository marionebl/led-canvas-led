{
  "name": "led-canvas-led",
  "version": "0.0.1",
  "description": "Example LED class to be used by https://github.com/marionebl/led-canvas",
  "main": "lib/led-canvas-led.js",
  "scripts": {
    "build": "npm run browserify && npm run uglify",
    "watch": "./node_modules/.bin/nodemon --exec \"npm run build\"",
    "serve": "./node_modules/.bin/static .",
    "browserify": "./node_modules/.bin/browserify ./lib/index.js -t 6to5-browserify -o ./dist/led-canvas-led.js",
    "uglify": "./node_modules/.bin/uglifyjs dist/led-canvas-led.js -o ./dist/led-canvas-led.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marionebl/led-canvas-led.git"
  },
  "keywords": [
    "led",
    "canvas",
    "led-board"
  ],
  "author": "Mario Nebl <hello@mario-nebl.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marionebl/led-canvas-led/issues"
  },
  "homepage": "https://github.com/marionebl/led-canvas-led",
  "devDependencies": {
    "6to5-browserify": "0.0.11",
    "browserify": "^6.1.0",
    "node-static": "^0.7.6",
    "nodemon": "^1.2.1",
    "uglify-js": "^2.4.15"
  }
}
